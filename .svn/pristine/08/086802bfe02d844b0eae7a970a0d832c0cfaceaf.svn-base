<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8"></meta>
  <meta http-equiv="X-UA-Compatible" content="IE=edge"></meta>
  <title>AdminLTE 2 | Data Tables</title>
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport"></meta>
  <link rel="stylesheet" href="../../css/bootstrap/dist/css/bootstrap.min.css"></link>
  <link rel="stylesheet" href="../../css/font-awesome/css/font-awesome.min.css"></link>
  <link rel="stylesheet" href="../../css/Ionicons/css/ionicons.min.css"></link>
  <link rel="stylesheet" href="../../css/datatables.net-bs/css/dataTables.bootstrap.min.css"></link>
  <link rel="stylesheet" href="../../css/dist/css/AdminLTE.min.css"></link>
  <link rel="stylesheet" href="../../css/dist/css/skins/_all-skins.min.css"></link>
  <link rel="stylesheet" type="text/css" href="../../css/fenlei.css"></link>
</head>
<body class="hold-transition skin-blue sidebar-mini">
<div class="wrapper">
<!--主页面左边菜单和上部分横条公共部分-->
<script src="../../tables/common/left.js"></script>
<!--结束-->
  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-xs-12">
       <div class="box" style="overflow: hidden;">
            <h3 class="box-title" style="margin-left: 30px;">系统技能维护</h3>
            <div class="box-header" > 
              <input type="text" placeholder="技能名称">
              <input type="text" placeholder="等级">
              <input type="text" placeholder="技能类型">
              <input type="button" value="查询">
                <button type="button" class="btn btn-block btn-info" style="width: 50px;display: block;float: right;margin-top: -30px;margin-left: 20px;">删除</button>
             <button type="button" id="edit" class="btn btn-block btn-info" style="width: 50px;display: block;float: right;margin-top: -30px;margin-left: 20px;">编辑</button>
               <button type="button" id="add" class="btn btn-block btn-info" style="width: 50px;display: block;float: right;margin-top: -30px;margin-left: 20px;">添加</button>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
              <table id="example2" class="table table-bordered table-hover">
                <thead>
                <tr>
                  <th>选择</th>
                  <th>技能ID</th>
                  <th>技能名称</th>
                  <th>技能类型</th>
                  <th>技能等级</th>
                  <th>录入数</th>
                  <th>积分兑换比例</th>
                  <th>准确率指标</th>
                  <th>练习题量</th>  
                  <th>练习通过指标</th>
                  <th>修炼题量</th> 
                 <th>修炼通过指标</th>
                </tr>
                </thead>
                <tbody>
                <tr>
                  <td><input type="checkbox"/></td>
                  <td>02001</td>
                  <td>简单版面</td>
                  <td>版面</td>
                  <td>初级</td>
                  <th>1</th>
                  <td>1000</td>
                  <td>95%</td>
                  <td>50</td>
                  <td>98%</td>
                  <th>20</th>
                  <th>100</th>
                </tr>
                </tbody>
                <tfoot>
                <tr>
                      <td><input type="checkbox"/></td>
                  <td>03001</td>
                  <td>简单数字</td>
                  <td>录入</td>
                  <td>入门级</td>
                  <th>2</th>
                  <td>100</td>
                  <td>98%</td>
                  <td>50</td>
                  <td>98%</td>
                  <th>26</th>
                  <th>100</th>
                </tr>
                </tfoot>  
                 <tfoot>
                <tr>
                      <td><input type="checkbox"/></td>
                  <td>02001</td>
                  <td>复杂地址</td>
                  <td>录入</td>
                  <td>高级</td>
                  <th>1</th>
                  <td>800</td>
                  <td>94%</td>
                  <td>60</td>
                  <td>98%</td>
                  <th>20</th>
                  <th>100</th>
                </tr>
                </tfoot>    
                 <tfoot>
                <tr>
                      <td><input type="checkbox"/></td>
                  <td>05001</td>
                  <td>简单纠错</td>
                  <td>纠错</td>
                  <td>中级</td>
                  <th>4</th>
                  <td>600</td>
                  <td>97%</td>
                  <td>60</td>
                  <td>94%</td>
                  <th>10</th>
                  <th>100</th>
                </tr>
                </tfoot>    
              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>
    <!-- /.content -->
  </div>
  <!-- Add the sidebar's background. This div must be placed
       immediately after the control sidebar -->
  <div class="control-sidebar-bg"></div>
</div>
<script src="../../js/jquery/dist/jquery.min.js"></script>
<script src="../../js/fastclick/lib/fastclick.js"></script>
<script src="../../js/adminlte.min.js"></script>
 <script type="text/javascript">  
    $('#add').click(function(){
      $("#MaskAdd").css("display","block");
      $("#maskRAdd").css("display","block");
    $(".QuXiaoButton").click(function(){//点击遮罩遮罩隐藏
      $("#MaskAdd").css("display","none");
    });
     $(".fa-times").click(function(){//点击遮罩遮罩隐藏
      $("#MaskAdd").css("display","none");
    });
    })
  </script>

<!--技能管理 系统技能维护 添加按钮-->
<div id="MaskAdd" class="Mask" >
  <div class="Box">
    <div class="Name">
      <div class="fa fa-times"  style="float: right;margin-right: 10px;font-size: 22px;color:#707070;"></div>
      系统技能维护新增
    </div>
    <div class="OneHeng"></div>
    <div id="maskRAdd" class="maskR">
          <div>
            <label >技能名称：</label>
            <input type="text" />
            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
            <label >技能类型：</label>
              <select>
                   <option value ="版面">版面</option>
                   <option value ="录入">录入</option>
                   <option value ="纠错">录入</option>
              </select>
          </div>

           <div>
            <label >技能等级：</label>
              <select>
                   <option value ="初级">初级</option>
                   <option value ="入门级">入门级</option>
                   <option value ="高级">高级</option>
              </select>
              &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
            <label>录入数：</label>
            <input type="text" />
          </div>

           <div >
            <label>积分兑换比例：</label>
            <input type="text" />
            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
            <label>准确率指标：</label>
            <input type="text" />
          </div>

           <div>
            <label>练习题量：</label>
            <input type="text" />
            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
            <label>练习通过指标：</label>
            <input type="text" />
          </div>

           <div >
            <label>修炼题量：</label>
            <input type="text" />
            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
            <label>修炼通过指标：</label>
            <input type="text" />
          </div>
</div>
        <div class="TwoHeng"> </div>
            <div >
           <button class="button">
            确定
           </button>
           <button  class="QuXiaoButton">
            取消
           </button>
            </div>
  </div>
 </div>
</div>
<!--技能管理添加按钮-->

<!--技能管理 系统技能维护 编辑按钮-->
<script type="text/javascript">  
    $('#edit').click(function(){
      $("#MaskEdit").css("display","block");
      $("#maskREdit").css("display","block");
    $(".QuXiaoButton").click(function(){//点击遮罩遮罩隐藏
      $("#MaskEdit").css("display","none");
    });
     $(".fa-times").click(function(){//点击遮罩遮罩隐藏
      $("#MaskEdit").css("display","none");
    });
    })
  </script>
<div id="MaskEdit" class="Mask" >
  <div class="Box">
    <div class="Name">
      <div class="fa fa-times"  style="float: right;margin-right: 10px;font-size: 22px;color:#707070;"></div>
      系统技能维护修改
    </div>
    <div class="OneHeng"></div>
    <div class="maskR" id="maskREdit">
          <div>
            <label >技能名称：</label>
            <input type="text" />
            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
            <label >技能类型：</label>
              <select disabled="disabled">
                   <option value ="版面">版面</option>
                   <option value ="录入">录入</option>
                   <option value ="纠错">录入</option>
              </select>
          </div>

           <div>
            <label >技能等级：</label>
              <select disabled="disabled">
                   <option value ="初级">初级</option>
                   <option value ="入门级">入门级</option>
                   <option value ="高级">高级</option>
              </select>
              &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
            <label>录入数：</label>
            <input type="text" />
          </div>

           <div >
            <label>积分兑换比例：</label>
            <input type="text" />
            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
            <label>准确率指标：</label>
            <input type="text" />
          </div>

           <div>
            <label>练习题量：</label>
            <input type="text" />
            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
            <label>练习通过指标：</label>
            <input type="text" />
          </div>

           <div >
            <label>修炼题量：</label>
            <input type="text" />
            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
            <label>修炼通过指标：</label>
            <input type="text" />
          </div>
</div>
        <div class="TwoHeng"> </div>
            <div >
           <button class="button">
            确定
           </button>
           <button  class="QuXiaoButton">
            取消
           </button>
            </div>
  </div>
 </div>
</div>
<!--技能管理添加按钮-->


