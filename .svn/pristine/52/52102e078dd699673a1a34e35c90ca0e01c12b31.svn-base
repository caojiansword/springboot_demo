              <input type="button" value="查询">
               <button type="button" id="edit" class="btn btn-block btn-info" style="width: 100px;display: block;float: right;margin-top: -30px;margin-right: 30px;">修改状态</button>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
              <table id="example2" class="table table-bordered table-hover">
                <thead>
                <tr>
                  <th>选择</th>
                  <th>用户名</th>
                  <th>手机号</th>
                  <th>技能ID</th>
                  <th>技能名称</th>
                  <th>状态</th>
                </tr>
                </thead>
                <tbody>
                <tr>
                  <td><input type="checkbox"/></td>
                  <td>张三</td>
                  <td>12536985852</td>
                  <td>0001</td>
                  <td>初级</td>
                  <th>1</th>
                </tr>
                </tbody>
                <tfoot>
                <tr>
                      <td><input type="checkbox"/></td>
                  <td>李四</td>
                  <td>1258963212</td>
                  <td>0002</td>
                  <td>入门级</td>
                  <th>0</th>
                </tr>
                </tfoot>  
                 <tfoot>
                <tr>
                      <td><input type="checkbox"/></td>
                  <td>赵武</td>
                  <td>1472583695852</td>
                  <td>0003</td>
                  <td>高级</td>
                  <th>1</th>
                </tr>
                </tfoot>      
              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>
    <!-- /.content -->
  </div>
  <!-- Add the sidebar's background. This div must be placed
       immediately after the control sidebar -->
  <div class="control-sidebar-bg"></div>
</div>
<script src="../../js/jquery/dist/jquery.min.js"></script>
<script src="../../js/fastclick/lib/fastclick.js"></script>
<script src="../../js/adminlte.min.js"></script>
<!--技能管理 系统技能维护 编辑按钮-->
<script type="text/javascript">  
    $('#edit').click(function(){
      $("#MaskEdit").css("display","block");
      $("#maskREdit").css("display","block");
    $(".QuXiaoButton").click(function(){//点击遮罩遮罩隐藏
      $("#MaskEdit").css("display","none");
    });
     $(".fa-times").click(function(){//点击遮罩遮罩隐藏
      $("#MaskEdit").css("display","none");
    });
    })
  </script>
<div id="MaskEdit" class="Mask" >
  <div class="Box">
    <div class="Name">
      <div class="fa fa-times"  style="float: right;margin-right: 10px;font-size: 22px;color:#707070;"></div>
      人员技能维护状态修改
    </div>
    <div class="OneHeng"></div>
    <div class="maskR" id="maskREdit">
           <div>
            <label>状态：</label>
                <select>
                   <option value ="可用">可用</option>
                   <option value ="不可用">不可用</option>
              </select>
          </div>
</div>
        <div class="TwoHeng"> </div>
            <div >
           <button class="button">
            确定
           </button>
           <button  class="QuXiaoButton">
            取消
           </button>
            </div>
  </div>
 </div>
</div>
