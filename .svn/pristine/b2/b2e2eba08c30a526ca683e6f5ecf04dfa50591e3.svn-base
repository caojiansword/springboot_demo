@charset "UTF-8";
/* CSS Document */
	.Mask{background:rgba(0,0,0,0.5);width: 100%;height:100%;position: fixed;z-index: 101;top:0;left:0;display:none;}
	.Box{width:800px;background:#fff;margin:0 auto;padding-bottom:60px;}
	.Name{font-size: 22px;text-align:left;margin-top:120px;color:#181818;margin-left: 15px;padding-top: 10px;}
	.maskL,.maskR{margin-top:30px;width:90%;margin-left:5%;}
	.maskL div,.maskR div{margin-bottom: 15px;}
	.Box label{margin-bottom:5px;color:#202020;width: 120px;text-align: right;}
	.Box input{width:25%;border:1px solid #CCCCFF;height: 35px;border-radius: 4px;padding-left: 20px;}
	.Box select{width:25%;border:1px solid #CCCCFF;height: 35px;border-radius: 4px;}
	.button{height:30px;color:#fff;border: none;font-size:18px;border-radius: 4px;background:#3c8dbc;float: right;margin-top: 15px;margin-left: 20px;margin-right: 15px;}
	.QuXiaoButton{height:30px;color:#fff;border: none;font-size:18px;border-radius: 4px;float: right;margin-top: 15px;}
	.OneHeng{border-top: 1px solid #dddddd;margin-top: 10px;}
	.TwoHeng{border-top: 1px solid #dddddd;}
	.Shuxian{width: 1px;height: 100%;background: #000;overflow: float;}
	.delete{font-size: 30px;color: red;margin-left: 80px;}

	/*用户管理 */
	#bd .right{float:right;width:100%;margin-left:-25em;}
	#bd .right .content{margin-left:270px;}
	#bd .left{width:200px;float:left;}
	.content input{width:100px;}
	.content input[type=checkbox]{margin-left: -20px;margin-top: -10px;}
	.mokuai{border: 1px dashed  red;}
	.mokuai input{margin-left: 10px;width: 110px;}
	.mokuai input[type=checkbox]{margin-left: -30px;margin-top: -10px;}
	.mokuai span{font-size: 22px;margin-left: 10px;}